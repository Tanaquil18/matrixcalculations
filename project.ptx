<?xml version="1.0" encoding="utf-8"?>
<!-- This file, the project manifest, provides the overall configuration for your PreTeXt project. To edit the content of your document, open `source/main.ptx`. See https://pretextbook.org/doc/guide/html/processing-CLI.html#cli-project-manifest. -->
<project ptx-version="2">
  <targets>
    <target name="web" format="html" >
      <stringparams html.css.extra="external/custom-styles.css" />
    </target>
    <target name="print" format="pdf" publication="publication-print.ptx" />
    <target name="worksheets" format="pdf" source="worksheets.ptx" publication="publication-print-worksheets.ptx"/>
    <target name="latex" format="latex"/>
    <target name="runestone" format="html" publication="publication-runestone.ptx" output-dir="published/matrixcalculations">
      <stringparams html.css.extra="external/custom-styles.css" />
    </target>
    <target name="worksheets-instructor" format="pdf" source="worksheets.ptx" publication="publication-worksheets-instructor.ptx"/>
  </targets>
</project>
