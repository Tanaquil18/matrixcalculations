<diagram dimensions="(300,300)" margins="5" xmlns="https://prefigure.org">
    <definition>f(x) = x</definition>
    <coordinates bbox="[-6.5,-6.5,6.5,6.5]">
    <axes hlabels="(1,1,1)" vlabels="(1,1,1)" xlabel="x" ylabel="y"/>

    <group at="graph-fpoints" outline="tactile">
        <graph at="graph-f" function='f' stroke="blue"/>
        <point at="point1" p="(1, f(1))" stroke="black" fill="blue" alignment="e"><m>(1,1)</m></point>
        <point at="point2" p="(3.14, f(3.14))" stroke="black" fill="blue" alignment="e"><m>(\pi,\pi)</m></point>
        <point at="point3" p="(-5, f(-5))" stroke="black" fill="blue" alignment="se"><m>(-5,-5)</m></point>
    </group>
    </coordinates>

